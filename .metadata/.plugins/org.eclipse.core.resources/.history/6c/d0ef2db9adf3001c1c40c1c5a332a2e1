package com.qa.main;

public class Flowchart3 {

	public static void main(String[] args) {

        for (int A = 100; A <= 200; A++) {
            if(A % 2 == 0) {
                System.out.println("-");
            }
            else{
            System.out.println("*");
            }
        }
        }
}

